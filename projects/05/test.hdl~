/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP test {
    IN in[15];
    OUT out;

    PARTS:
    // Put your code here:
    And(a=in[0] ,b=in[14] ,out=out);
}
